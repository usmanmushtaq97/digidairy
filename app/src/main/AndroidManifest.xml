<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tss.digidairy">
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Muguri"
        android:usesCleartextTraffic="true">
        <activity android:name="com.tss.digidairy.app.Activities.DetailsOrders"></activity>
        <activity android:name="com.tss.digidairy.app.Activities.ProductByCategoryIdActivity" />
        <activity android:name="com.tss.digidairy.app.Activities.SubCategories" />
        <activity android:name="com.tss.digidairy.app.Activities.ProductDetails" />
        <activity android:name="com.tss.digidairy.app.Activities.LoginActivity" />
        <activity android:name="com.tss.digidairy.app.Activities.GetLoginSignActivity" />
        <activity android:name="com.tss.digidairy.app.Activities.GetMobileNumber" />
        <activity android:name="com.tss.digidairy.app.Activities.VerifyOtpActivity" />
        <activity android:name="com.tss.digidairy.app.Activities.SignUp" />
        <activity android:name="com.tss.digidairy.app.Activities.HomeActivity" />
        <activity android:name="com.tss.digidairy.app.Activities.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>